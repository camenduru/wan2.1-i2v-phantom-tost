{
  "$schema": "http://json-schema.org/draft-04/hyper-schema#",
  "type": "object",
  "properties": {
    "readme": {
      "type": "string",
      "title": "Wan 2.1 Phantom I2V: Subject-consistent video generation via cross-modal alignment",
      "author": "Lijie Liu, Tianxiang Ma, Bingchuan Li, Zhuowei Chen, Jiawei Liu, Gen Li, Siyu Zhou, Qian He, Xinglong Wu",
      "code": "https://github.com/Phantom-video/Phantom",
      "paper": "https://arxiv.org/abs/2502.11079",
      "page": "https://phantom-video.github.io/Phantom/",
      "tags": [
        "Image to Video"
      ],
      "widget": "readme"
    },
    "upload1": {
      "type": "string",
      "widget": "upload",
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "input_image1"
    },
    "input_image1": {
      "type": "string",
      "description": "Input Image 1 URL",
      "widget": "textarea"
    },
    "upload2": {
      "type": "string",
      "widget": "upload",
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "input_image2"
    },
    "input_image2": {
      "type": "string",
      "description": "Input Image 2 URL",
      "widget": "textarea"
    },
    "upload3": {
      "type": "string",
      "widget": "upload",
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "input_image3"
    },
    "input_image3": {
      "type": "string",
      "description": "Input Image 3 URL",
      "widget": "textarea"
    },
    "upload4": {
      "type": "string",
      "widget": "upload",
      "upload_url": "https://tost.ai/api/jobs/upload",
      "property": "input_image4"
    },
    "input_image4": {
      "type": "string",
      "description": "Input Image 4 URL",
      "widget": "textarea"
    },
    "positive_prompt": {
      "type": "string",
      "description": "Positive Prompt",
      "widget": "textarea"
    },
    "negative_prompt": {
      "type": "string",
      "description": "Negative Prompt",
      "widget": "hidden"
    },
    "width": {
      "type": "integer",
      "widget": "select",
      "description": "Width",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1024",
          "enum": [
            1024
          ]
        },
        {
          "description": "1280",
          "enum": [
            1280
          ]
        }
      ],
      "default": 1024
    },
    "height": {
      "type": "integer",
      "widget": "select",
      "description": "Height",
      "oneOf": [
        {
          "description": "576",
          "enum": [
            576
          ]
        },
        {
          "description": "720",
          "enum": [
            720
          ]
        },
        {
          "description": "1024",
          "enum": [
            1024
          ]
        },
        {
          "description": "1280",
          "enum": [
            1280
          ]
        }
      ],
      "default": 576
    },
    "length": {
      "type": "integer",
      "description": "Length",
      "widget": "hidden"
    },
    "batch_size": {
      "type": "integer",
      "description": "Batch Size",
      "widget": "hidden"
    },
    "shift": {
      "type": "integer",
      "description": "Shift"
    },
    "cfg": {
      "type": "number",
      "description": "CFG"
    },
    "sampler_name": {
      "type": "string",
      "description": "Sampler Name"
    },
    "scheduler": {
      "type": "string",
      "description": "Scheduler"
    },
    "steps": {
      "type": "integer",
      "description": "Steps",
      "widget": "hidden"
    },
    "seed": {
      "type": "integer",
      "description": "Seed (if 0 random)"
    },
    "fps": {
      "type": "integer",
      "description": "FPS"
    },
    "filename_prefix": {
      "type": "string",
      "description": "Filename Prefix",
      "widget": "hidden"
    },
    "example": {
      "type": "string",
      "image1": "https://files.catbox.moe/is6hkt.jpg",
      "image2": "https://files.catbox.moe/sd01ul.jpg",
      "image3": "https://files.catbox.moe/whpkw0.png",
      "image4": "https://files.catbox.moe/mgantw.mp4",
      "video_aspect_ratio": "60%",
      "line1": "Video: 1280x720 pixels, 5 seconds | Gen Time: ~6 minutes",
      "line2": "Video: 1024x576 pixels, 5 seconds | Gen Time: ~3 minutes",
      "widget": "example"
    }
  },
  "buttons": [
    {
      "id": "enter",
